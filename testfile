pipeline {
    agent any

     stages {
        stage('gitjob') {
            steps {
                echo 'pulling the code from git hub'
                git 'https://github.com/SpoorthiKL/Calculatorjava.git'
            }
        }
        stage('buildjob') {
            steps {
                echo 'Building the job'
                sh 'mvn compile'
            }
        }
        stage('testjob') {
            steps {
                echo 'Testing the job'
                sh 'mvn test'
            }
        }
        stage('deployjob') {
            steps {
                echo 'Depoying the job'
                sh 'mvn install'
            }
        }
    }
}
